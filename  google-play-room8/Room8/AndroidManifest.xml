<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.finalapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <permission
        android:name="com.udi.permission.UDI_PERMISSION"
        android:protectionLevel="normal" />
    
    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_OWNER_DATA" />
    <uses-permission android:name="android.permission.READ_OWNER_DATA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="com.udi.permission.UDI_PERMISSION" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:name="com.example.finalapp.App"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.login.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Black.NoTitleBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.login.RegistrationActivity"
            android:parentActivityName="com.example.login.MainActivity"
            android:screenOrientation="portrait" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.finalapp.MainActivity" />
        </activity>
        <activity
            android:name="com.example.finalapp.SplitActionBarActivity"
            android:label="@string/title_activity_split_action_bar"
            android:screenOrientation="portrait"
            android:uiOptions="splitActionBarWhenNarrow" >
            <meta-data
                android:name="android.support.UI_OPTIONS"
                android:value="splitActionBarWhenNarrow" />
        </activity>
        <activity
            android:name="com.example.home.HomeActivity"
            android:label="@string/title_activity_home"
            android:screenOrientation="portrait"
            android:uiOptions="splitActionBarWhenNarrow" >
        </activity>
        <activity
            android:name="com.example.budget.BudgetManagerActivity"
            android:label="@string/title_activity_budget_manager"
            android:screenOrientation="portrait"
            android:uiOptions="splitActionBarWhenNarrow" >
        </activity>

        <service android:name="com.parse.PushService" />

        <receiver android:name="com.parse.ParseBroadcastReceiver" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.intent.action.USER_PRESENT" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="com.example.home.MyCustomReceiver"
            android:permission="com.udi.permission.UDI_PERMISSION" >
            <intent-filter>
                <action android:name="com.example.UPDATE_IS_HOME" />
            </intent-filter>
        </receiver>
        
        <activity
            android:name="com.example.home.CamActivity"
            android:label="@string/title_activity_cam"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.login.PushApartmentActivity"
            android:label="@string/title_activity_push_apartment"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.login.PushApartmentAdminActivity"
            android:label="@string/title_activity_push_apartment_admin"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.bills.BillsActivity"
            android:label="@string/title_activity_bills"
            android:screenOrientation="portrait"
            android:uiOptions="splitActionBarWhenNarrow" >
        </activity>
        <activity
            android:name="com.example.bills.Edit_new_bill_categoryActivity"
            android:label="@string/title_activity_edit_new_bill"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.bills.Bill_InformationActivity"
            android:label="@string/title_activity_bill__information"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.bills.MultiChoiceDialog"
            android:label="@string/title_activity_multi_choice_dialog"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.finalapp.DialogActivity"
            android:label="@string/title_activity_dialog"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.shopping.ShoppingListActivity"
            android:label="@string/title_activity_shopping_list"
            android:screenOrientation="portrait"
            android:uiOptions="splitActionBarWhenNarrow" >
        </activity>
        <activity
            android:name="com.example.shopping.ShoppingEditActivity"
            android:label="@string/title_activity_shopping_edit"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.home.HomeAdminActivity"
            android:label="@string/title_activity_home_continue"
            android:screenOrientation="portrait"
            android:uiOptions="splitActionBarWhenNarrow" >
        </activity>
        <activity
            android:name="com.example.sticky_notes.StickyNoteActivity"
            android:label="@string/title_activity_sticky_note"
            android:screenOrientation="portrait"
            android:uiOptions="splitActionBarWhenNarrow" >
        </activity>
        <activity
            android:name="com.example.sticky_notes.Edit_new_noteActivity"
            android:label="@string/title_activity_edit_new_note"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.bills.BillPaymentsListActivity"
            android:label="@string/title_activity_bill_payments_list"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.budget.BudgetAddActivity"
            android:label="@string/title_activity_budget_add"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.budget.BudgetEditActivity"
            android:label="@string/title_activity_budget_edit"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.budget.BudgetGraphActivity"
            android:label="@string/title_activity_budget_graph"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.gallery.GalleryActivity"
            android:label="@string/title_activity_gallery"
            android:uiOptions="splitActionBarWhenNarrow"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>